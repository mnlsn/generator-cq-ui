{
  "name": "grunt-cq-build",
  "private": true,
  "version": "0.2.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnlsn/grunt-cq-build"
  },
  "description": "Watch files edited and automatically deploy them to CRX",
  "devDependencies": {
    "grunt": "~0.4.2",
    "grunt-shell": "~0.6.4",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-macreload": "~0.1.3",
    "load-grunt-tasks": "~0.2.1",
    "time-grunt": "~0.2.8",
    "grunt-slung": "git://github.com/mnlsn/grunt-slung"
  },
  "options": {
    "host": "<%= cqHost %>",
    "author": "<%= cqAuthor %>",
    "publish": "<%= cqPublish %>",
    "user": "<%= cqLoginName %>",
    "password": "<%= cqLoginPW %>",
    "project": "<%= cqProject %>",
    "cqroot": "<%= cqRoot %>",
    "mvnpublish": "<%= mvnPublish %>",
    "mvnauthor": "<%= mvnAuthor %>",
    "browser": "<%= devBrowser %>"
  }
}
